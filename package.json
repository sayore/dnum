{
  "name": "@sayore/dnum",
  "version": "0.8.9",
  "description": "High-precision dimensional numbers for infinite scales and micro-precision.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sayore/dnum.git"
  },
  "bugs": {
    "url": "https://github.com/sayore/dnum/issues"
  },

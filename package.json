    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
